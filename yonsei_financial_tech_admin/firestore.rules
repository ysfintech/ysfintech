rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /paper/{document=**} {
    	allow read, write;
    }
    match /work/{document=**} {
      allow read, write;
    }
    match /{document=**} {
    	allow read;
      allow write : if request.auth.uid == 'dfa4do4d5fgxEd2bo1AptWA57S82';
    }
  }
}